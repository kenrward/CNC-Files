<openscam>
  <!-- Note, all values are in mm regardless of 'units' option. -->
  <!-- NC Files -->
  <nc-files>
    StoolwTabs.nc
  </nc-files>

  <!-- Renderer -->
  <resolution v='0.845071'/>
  <resolution-mode v='HIGH'/>

  <!-- Workpiece -->
  <automatic-workpiece v='false'/>
  <workpiece-max v='(508,330,0)'/>
  <workpiece-min v='(0,0,-18)'/>

  <tool_table>
    <tool length='2' number='1' radius='0.125' shape='CYLINDRICAL' units='INCH'>
      1/4 inche mill
    </tool>
  </tool_table>
</openscam>